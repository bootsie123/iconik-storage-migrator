{
  "name": "iconik-proxy-migrator",
  "version": "1.0.0",
  "description": "Migrates proxies in Iconik to another storage location",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsc-watch --onSuccess \"node dist/server.js\"",
    "build": "tsc",
    "lint": "eslint \"**/*.{js,ts}\"",
    "lint:fix": "eslint --fix \"**/*.{js,ts}\"",
    "docker:build": "docker build -t iconik-proxy-migrator .",
    "docker:run": "docker run -d -p 3000:3000 --name iconik-proxy-migrator iconik-proxy-migrator",
    "postdocker:build": "docker image prune -f --filter label=stage=intermediate"
  },
  "author": "John Arrandale",
  "license": "ISC",
  "homepage": "https://github.com/bootsie123/iconik-proxy-migrator",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bootsie123/iconik-proxy-migrator.git"
  },
  "bugs": {
    "url": "https://github.com/bootsie123/iconik-proxy-migrator/issues"
  },
  "keywords": [
    "iconik",
    "migration"
  ],
  "exports": {
    ".": "./dist/server.js"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.7",
    "@types/node-fetch": "^2.6.11",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-recommended": "^4.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "lint-staged": "^15.2.4",
    "prettier": "^3.3.2",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@bull-board/express": "^5.20.2",
    "async-sema": "^3.1.1",
    "bullmq": "^5.8.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fast-safe-stringify": "^2.1.1",
    "iconik-typescript": "file:../iconik-typescript/typescript-client",
    "node-fetch": "^2.7.0",
    "winston": "^3.13.0"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix"
  }
}
